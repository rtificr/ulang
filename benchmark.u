let makeState = fn() {
    let v = 0

    {
        ["next"] = fn() {
            v = v + 1
            v
        },
        ["get"] = fn() v,
        ["ref"] = fn() &v
    }
}

let goon = fn(x: number) x**2
let state = makeState()
let state2 = makeState()
print(state["next"]())
print(state2["next"]())
print(state["get"]())
print(state2["get"]())
